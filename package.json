{
  "name": "mxd.codes",
  "description": "Personal Website",
  "author": "Max Dietrich",
  "version": "2.1.0",
  "license": "MIT",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "deploy": "git pull && npm run build && pm2 restart mxd-codes-frontend",
    "export": "next export",
    "analyze": "cross-env ANALYZE=true next build",
    "generate-manifest": "node ./lib/manifest.js",
    "img-optimize": "py ./utils/resize_images.py",
    "lint": "eslint --fix .",
    "format": "prettier --write src/**/*.js --config .prettierrc.json",
    "count": "cloc --exclude-ext=svg --exclude-dir=node_modules,.next --json src/ pages/ styles/ public/ . > src/data/count_total.json",
    "webmention": "webmention https://mxd.codes/feed.xml --limit 0 --send",
    "test": "npx next lint"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "@next/bundle-analyzer": "^12.0.10",
    "@prisma/client": "3.9.1",
    "@remy/webmention": "^1.4.5",
    "@socialgouv/matomo-next": "^1.2.2",
    "axios": "^0.25.0",
    "babel-eslint": "^10.1.0",
    "cloc": "^2.8.0",
    "cross-env": "^7.0.3",
    "date-fns": "2.28.0",
    "globby": "^13.1.1",
    "js-cookie": "^3.0.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "next": "^12.0.10",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-pwa": "^5.2.24",
    "object-hash": "^2.2.0",
    "ol": "^6.12.0",
    "preact": "^10.6.5",
    "preact-compat": "^3.19.0",
    "preact-render-to-string": "^5.1.19",
    "prettier": "2.5.1",
    "prismjs": "^1.26.0",
    "puppeteer": "^13.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-emoji-render": "^1.2.4",
    "react-icons": "^4.2.0",
    "react-markdown": "^8.0.0",
    "react-router-dom": "^6.2.1",
    "react-tooltip": "^4.2.21",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-slug": "^7.0.1",
    "sharp": "^0.30.0",
    "showdown": "^2.0.0-alpha",
    "slugify": "^1.6.5",
    "string-similarity": "^4.0.4",
    "styled-components": "5.3.3",
    "styled-media-query": "^2.1.2",
    "swr": "^1.2.1",
    "topojson-client": "^3.1.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.0.10",
    "eslint-config-nextjs": "1.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.5.5"
  }
}
