{
  "name": "mxd.codes",
  "description": "Personal Website",
  "author": "Max Dietrich",
  "version": "2.2.0",
  "license": "MIT",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "deploy": "git pull && npm run build && pm2 restart mxd-codes-frontend",
    "export": "next export",
    "analyze": "cross-env ANALYZE=true next build",
    "img-optimize": "py ./utils/resize_images.py",
    "format": "prettier --write .",
    "count": "cloc --exclude-ext=svg --exclude-dir=node_modules,.next --json src/ pages/ styles/ public/ . > src/data/count_total.json",
    "webmention": "webmention https://mxd.codes/feed.xml --limit 0 --send",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "@next/bundle-analyzer": "^12.3.4",
    "@prisma/client": "3.13.0",
    "@remy/webmention": "^1.4.5",
    "@socialgouv/matomo-next": "^1.6.0",
    "axios": "^0.27.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "cloc": "^2.11.0",
    "cross-env": "^7.0.3",
    "date-fns": "2.28.0",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.1",
    "jspdf": "^2.5.1",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.21",
    "next": "^13.1.4",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-mdx-remote": "^4.3.0",
    "next-pwa": "^5.5.2",
    "object-hash": "^3.0.0",
    "ol": "^6.14.1",
    "preact": "^10.7.2",
    "preact-compat": "^3.19.0",
    "preact-render-to-string": "^5.2.0",
    "prettier": "2.6.2",
    "prismjs": "^1.28.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-emoji-render": "^1.2.4",
    "react-icons": "^4.7.1",
    "react-markdown": "^8.0.4",
    "react-modal-image": "^2.6.0",
    "react-router-dom": "^6.6.2",
    "react-tooltip": "^4.5.1",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-slug": "^7.0.1",
    "sharp": "^0.31.3",
    "showdown": "^2.1.0",
    "slugify": "^1.6.5",
    "string-similarity": "^4.0.4",
    "styled-components": "5.3.5",
    "styled-media-query": "^2.1.2",
    "swr": "^1.3.0",
    "topojson-client": "^3.1.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.3.4",
    "eslint-config-nextjs": "1.0.7",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
