{
  "name": "mxd.codes",
  "description": "Personal Website",
  "author": "Max Dietrich",
  "version": "2.2.0",
  "license": "MIT",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "deploy": "git pull && npm run build && pm2 restart mxd-codes-frontend",
    "export": "next export",
    "analyze": "cross-env ANALYZE=true next build",
    "img-optimize": "py ./utils/resize_images.py",
    "format": "prettier --write .",
    "count": "cloc --exclude-ext=svg --exclude-dir=node_modules,.next --json src/ pages/ styles/ public/ . > src/data/count_total.json",
    "webmention": "webmention https://mxd.codes/feed.xml --limit 0 --send",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "@next/bundle-analyzer": "^14.0.4",
    "@remy/webmention": "^1.4.5",
    "@socialgouv/matomo-next": "^1.6.0",
    "@use-it/event-listener": "^0.1.7",
    "axios": "^1.6.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "cloc": "^1.98.0-cloc",
    "create-hmac": "^1.1.7",
    "cross-env": "^7.0.3",
    "date-fns": "^3.2.0",
    "eslint-config-next": "^14.0.4",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.1",
    "jspdf": "^2.5.1",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.21",
    "next": "^14.0.4",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-mdx-remote": "^4.3.0",
    "next-pwa": "^5.5.2",
    "object-hash": "^3.0.0",
    "ol": "^8.2.0",
    "prettier": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-emoji-render": "^2.0.1",
    "react-icons": "^5.0.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.6.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.25.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "remark-slug": "^7.0.1",
    "showdown": "^2.1.0",
    "slugify": "^1.6.5",
    "string-similarity": "^4.0.4",
    "styled-components": "^6.1.8",
    "styled-media-query": "^2.1.2",
    "swr": "^2.2.4",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
